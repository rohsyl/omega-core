{
    "name": "rohsyl/omega-core",
    "description": "Omega CMS Core",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "rohsyl",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.4",
        "tightenco/ziggy": "^1.3",
        "rohsyl/laravel-acl": "^1.1",
        "rohsyl/omega-plugins-bundle": "dev-master",
        "spatie/laravel-mix-preload": "^1.2",
        "rohsyl/laravel-advanced-query-filter": "dev-master",
        "wire-elements/modal": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "rohsyl\\OmegaCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\OmegaCore\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\OmegaCore\\ServiceProvider"
            ],
            "aliases": {
                "Form": "Collective\\Html\\FormFacade",
                "Entity": "rohsyl\\OmegaCore\\Utils\\Overt\\Facades\\Entity"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../omega-plugins-bundle"
        },
        {
            "type": "path",
            "url": "../laravel-advanced-query-filter"
        }
    ]
}
